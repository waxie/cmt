{% load cmts_extras %}
{% store /usr/sara/etc/support.nodes as output %}
{% noblanklines %}
{% use HardwareUnit with 'cluster__name=Lisa' as lisa_hosts %}
    {% for hw in lisa_hosts %}
            {% for i in hw.interfaces.all %}
                {% if i.network.name == 'lisa consoles' and "PowerEdge" in hw.specifications.name %} 
{{ i.fqdn }}
                {% endif %}
            {% endfor %}
    {% endfor %}
{% endnoblanklines %}
