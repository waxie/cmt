for node in 6 7; do cmt-sara --print machine id=fileservern${node} >> machines.fileservers; done
for node in subtrac2 ftp ganglia ganglia-ng hpc license subtrac; do cmt-sara --print machine id=hpcserver${node} >> machines.hpcservers; done
for node in n1 n2 n3 n4 n5 n6 install; do cmt-sara --print machine id=ldap${node} >> machines.ldap; done
